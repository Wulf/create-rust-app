{
  "name": "create-rust-app",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "dotenv": "^16.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.7.0",
    "typescript": "^4.9.4",
    "web-vitals": "^3.1.1"
  },
  "scripts": {
